@model List<Asos.FashionEmergency.Web.Controllers.Product>
    
<ul>
    @foreach (var product in Model)
    {
        <li>
            <h2>@product.Name</h2>
            <p>@product.Description</p>
            <p>£@product.Price @product.Availability HR</p>
            <img src="@product.ImageUrl" />
        </li>
    }
</ul>