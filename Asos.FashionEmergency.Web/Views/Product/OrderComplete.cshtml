@model Asos.FashionEmergency.Web.Controllers.OrderCompleteViewModel

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "ASOS ASAP - Order Complete";
}
@section Styles {
    <link href="@Url.Content("~/stylesheets/view-products.css")" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/stylesheets/map.css")" rel="stylesheet" type="text/css" />
}
@section Scripts {
    <script type="text/javascript">
        var storeName = '@Model.StoreName';
        var storePostcode = '@Model.StorePostcode';
        var destinationPostcode = '@Model.DestinationPostcode';
    </script>
    <script src="@Url.Content("~/scripts/map.js")"></script>
    <script src="@Url.Content("~/scripts/order-complete.js")"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDDejyl47XopChNRYsmWPNEUl1NtYGtG7k&callback=initMap" async defer></script>
}
<h2>Order Confirmed</h2>
<p>Your order will be delivered from <strong>@Model.StoreName</strong>.</p>
<div id="map"></div>